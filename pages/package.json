{
  "name": "pages",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "concurrently -k 'yarn:dev:*'",
    "dev:relay": "yarn generate --watch",
    "dev:vite": "vite",
    "generate": "relay-compiler",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mantine/colors-generator": "7.10.2",
    "@mantine/core": "7.10.2",
    "@mantine/form": "7.10.2",
    "@mantine/hooks": "7.10.2",
    "@mantine/notifications": "7.10.2",
    "@sentry/react": "7.99.0",
    "@tabler/icons-react": "2.47.0",
    "chroma-js": "2.4.2",
    "date-fns": "3.6.0",
    "idb": "8.0.0",
    "immer": "10.1.1",
    "jotai": "2.8.3",
    "radash": "11.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-relay": "16.2.0",
    "react-router-dom": "6.23.1",
    "react-use-promise": "0.5.0",
    "relay-runtime": "17.0.0",
    "swr": "2.2.5",
    "ts-array-length": "0.1.2"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "2.18.0",
    "@swc-jotai/debug-label": "0.1.0",
    "@swc-jotai/react-refresh": "0.1.0",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-relay": "16.0.6",
    "@vitejs/plugin-react-swc": "3.5.0",
    "babel-plugin-relay": "17.0.0",
    "concurrently": "8.2.2",
    "postcss": "8.4.38",
    "postcss-preset-mantine": "1.15.0",
    "postcss-simple-vars": "7.0.1",
    "relay-compiler": "17.0.0",
    "typescript": "5.3.3",
    "vite": "5.3.1",
    "vite-plugin-chunk-split": "0.5.0",
    "vite-plugin-pwa": "0.20.0",
    "vite-plugin-relay": "2.1.0"
  },
  "engines": {
    "node": ">=20"
  },
  "relay": {
    "src": "./src",
    "artifactDirectory": "./src/__generated__",
    "schema": "./schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "customScalars": {
      "DateTime": "string"
    },
    "noFutureProofEnums": true
  }
}
