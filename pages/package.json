    "date-fns": "2.30.0",
    "jotai": "2.4.2",
    "js-yaml": "4.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-relay": "15.0.0",
    "react-router-dom": "6.16.0",
    "react-use-promise": "0.5.0",
    "relay-runtime": "15.0.0",
    "swr": "2.2.2",
