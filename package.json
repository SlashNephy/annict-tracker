{
  "name": "annict-tracker",
  "version": "1.0.0",
  "author": "SlashNephy",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/annict-tracker"
  },
  "license": "MIT",
  "scripts": {
    "tsc": "tsc --noEmit",
    "build": "next build",
    "dev": "next dev",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "concurrently -n generate: -m 1 'yarn:generate:*'",
    "generate:curl": "curl -o graphql/generated/schema.graphql https://raw.githubusercontent.com/kiraka/annict-web/main/app/graphql/beta/schema.graphql",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "start": "next start"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/server": "11.10.0",
    "@kawaiioverflow/arm": "0.102.0",
    "@mantine/core": "5.4.1",
    "@mantine/hooks": "5.4.1",
    "@mantine/next": "5.4.1",
    "@mantine/notifications": "5.4.1",
    "@slashnephy/typescript-extension": "0.2.3",
    "@tabler/icons": "1.101.0",
    "@tanstack/react-query": "4.10.1",
    "caching-decorator": "1.0.2",
    "date-fns": "2.29.3",
    "fast-xml-parser": "4.0.10",
    "graphql-request": "4.3.0",
    "js-yaml": "4.1.0",
    "next": "12.3.1",
    "next-auth": "4.12.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.6",
    "@graphql-codegen/import-types-preset": "2.2.4",
    "@graphql-codegen/typescript": "2.7.4",
    "@graphql-codegen/typescript-graphql-request": "4.5.6",
    "@graphql-codegen/typescript-operations": "2.5.4",
    "@slashnephy/eslint-config": "0.2.6",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/js-yaml": "4.0.5",
    "@types/node": "17.0.25",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "concurrently": "7.4.0",
    "eslint": "8.25.0",
    "graphql": "16.6.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": "16"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/no-unsafe-call": "off",
      "@typescript-eslint/no-unsafe-member-access": "off",
      "@typescript-eslint/no-unsafe-return": "off",
      "@typescript-eslint/no-unsafe-argument": "off",
      "@typescript-eslint/restrict-template-expressions": "off"
    }
  }
}
