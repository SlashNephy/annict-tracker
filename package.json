{
  "name": "annict-tracker",
  "version": "1.0.0",
  "author": "SlashNephy",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/annict-tracker"
  },
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "concurrently -n generate: 'yarn:generate:*'",
    "generate:graphql": "concurrently -n graphql: -m 1 'yarn:graphql:*'",
    "graphql:annict:codegen": "graphql-codegen --config graphql/annict/codegen.yml",
    "graphql:annict:schema": "curl -o graphql/annict/schema.graphql https://raw.githubusercontent.com/kiraka/annict-web/main/app/graphql/beta/schema.graphql",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "start": "next start",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/server": "11.10.0",
    "@mantine/core": "5.8.3",
    "@mantine/hooks": "5.8.3",
    "@mantine/next": "5.8.3",
    "@mantine/notifications": "5.8.3",
    "@slashnephy/typescript-extension": "0.2.3",
    "@tabler/icons": "1.113.0",
    "@tanstack/react-query": "4.18.0",
    "@vercel/analytics": "0.1.5",
    "cached_property": "0.1.2",
    "date-fns": "2.29.3",
    "fast-xml-parser": "4.0.12",
    "graphql-request": "5.0.0",
    "js-yaml": "4.1.0",
    "next": "13.0.5",
    "next-auth": "4.17.0",
    "node-fetch": "3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "0.7.6",
    "recoil-persist": "4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/import-types-preset": "2.2.6",
    "@graphql-codegen/typescript": "2.8.5",
    "@graphql-codegen/typescript-graphql-request": "4.5.8",
    "@graphql-codegen/typescript-operations": "2.5.10",
    "@slashnephy/eslint-config": "0.2.24",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/js-yaml": "4.0.5",
    "@types/node": "17.0.25",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "concurrently": "7.6.0",
    "eslint": "8.29.0",
    "graphql": "16.6.0",
    "prettier": "2.8.1",
    "typescript": "4.9.3"
  },
  "packageManager": "yarn@3.3.0",
  "engines": {
    "node": "16"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
