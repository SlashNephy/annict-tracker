{
  "name": "anime-calendar",
  "version": "1.0.0",
  "author": "SlashNephy",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/anime-calendar"
  },
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "concurrently -n generate: -m 1 'yarn:generate:*'",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:introspect": "curl -o graphql/generated/schema.graphql https://raw.githubusercontent.com/kiraka/annict-web/main/app/graphql/beta/schema.graphql",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "start": "next start"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/server": "11.10.0",
    "@kawaiioverflow/arm": "0.102.0",
    "@mantine/core": "5.4.1",
    "@mantine/hooks": "5.4.1",
    "@mantine/next": "5.4.1",
    "@mantine/notifications": "5.4.1",
    "@tabler/icons": "1.100.0",
    "@tanstack/react-query": "4.10.1",
    "date-fns": "2.29.3",
    "fast-xml-parser": "4.0.10",
    "graphql-request": "4.3.0",
    "js-yaml": "4.1.0",
    "next": "12.3.1",
    "node-fetch": "3.2.10",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.4",
    "@graphql-codegen/import-types-preset": "2.2.3",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-graphql-request": "4.5.5",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@slashnephy/eslint-config": "0.2.6",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/js-yaml": "4.0.5",
    "@types/node": "17.0.25",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "concurrently": "7.4.0",
    "eslint": "8.22.0",
    "graphql": "16.6.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": "18.8.0"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
