{
  "name": "annict-tracker",
  "version": "1.0.0",
  "author": "SlashNephy",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/annict-tracker"
  },
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "concurrently -n generate: 'yarn:generate:*'",
    "generate:graphql": "concurrently -n graphql: -m 1 'yarn:graphql:*'",
    "graphql:annict:codegen": "graphql-codegen --config graphql/annict/codegen.ts",
    "graphql:annict:schema": "curl -o graphql/annict/schema.graphql https://raw.githubusercontent.com/kiraka/annict-web/main/app/graphql/beta/schema.graphql",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "webhint": "hint http://localhost:3000"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@mantine/core": "5.10.5",
    "@mantine/hooks": "5.10.5",
    "@mantine/next": "5.10.5",
    "@mantine/notifications": "5.10.5",
    "@slashnephy/typescript-extension": "0.2.3",
    "@tabler/icons-react": "2.25.0",
    "@tanstack/react-query": "4.29.19",
    "@vercel/analytics": "1.0.1",
    "cached_property": "0.1.2",
    "date-fns": "2.30.0",
    "fast-xml-parser": "4.2.5",
    "graphql-request": "6.0.0",
    "js-yaml": "4.1.0",
    "next": "13.4.10",
    "next-auth": "4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "0.7.7",
    "recoil-persist": "4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@slashnephy/eslint-config": "0.5.3",
    "@slashnephy/prettier-config": "0.1.25",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.16.19",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "concurrently": "8.2.0",
    "eslint": "8.44.0",
    "eslint-plugin-deprecation": "1.4.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-unused-imports": "3.0.0",
    "graphql": "16.7.1",
    "hint": "7.1.10",
    "prettier": "3.0.0",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "overrides": [
      {
        "files": "graphql/annict/codegen.ts",
        "parserOptions": {
          "project": "./tsconfig.node.json"
        },
        "rules": {
          "import/no-default-export": "off"
        }
      }
    ]
  }
}
