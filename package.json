{
  "name": "annict-tracker",
  "version": "1.0.0",
  "author": "SlashNephy",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/annict-tracker"
  },
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "concurrently -n generate: 'yarn:generate:*'",
    "generate:graphql": "concurrently -n graphql: -m 1 'yarn:graphql:*'",
    "graphql:annict:codegen": "graphql-codegen --config graphql/annict/codegen.ts",
    "graphql:annict:schema": "curl -o graphql/annict/schema.graphql https://raw.githubusercontent.com/kiraka/annict-web/main/app/graphql/beta/schema.graphql",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "webhint": "hint http://localhost:3000"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@mantine/core": "6.0.19",
    "@mantine/hooks": "6.0.19",
    "@mantine/next": "6.0.19",
    "@mantine/notifications": "6.0.19",
    "@slashnephy/typescript-extension": "0.2.3",
    "@tabler/icons-react": "2.32.0",
    "@tanstack/react-query": "4.33.0",
    "@vercel/analytics": "1.0.2",
    "cached_property": "0.1.3",
    "date-fns": "2.30.0",
    "fast-xml-parser": "4.2.7",
    "graphql-request": "6.1.0",
    "js-yaml": "4.1.0",
    "next": "13.4.19",
    "next-auth": "4.23.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "0.7.7",
    "recoil-persist": "5.1.0",
    "ts-array-length": "0.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@slashnephy/eslint-config": "1.0.6",
    "@slashnephy/prettier-config": "0.1.27",
    "@types/js-yaml": "4.0.5",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "concurrently": "8.2.1",
    "eslint": "8.47.0",
    "graphql": "16.8.0",
    "hint": "7.1.10",
    "prettier": "3.0.2",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.2",
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
