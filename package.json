{
  "name": "animetl-bot",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/animetl-bot",
  "author": "SlashNephy",
  "scripts": {
    "setup": "yarn graphql:download && yarn graphql:generate",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "ts-node src/ts/main.ts",
    "dev": "nodemon src/ts/main.ts",
    "webpack": "webpack",
    "webpack:watch": "webpack -w",
    "graphql:download": "curl -o src/graphql/schema.graphql https://raw.githubusercontent.com/kiraka/annict-web/main/app/graphql/beta/schema.graphql",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:eslint": "eslint",
    "lint:prettier": "prettier --check .",
    "format": "concurrently 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/import-types-preset": "2.2.3",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-graphql-request": "4.5.4",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@types/js-yaml": "4.0.5",
    "@types/node": "17.0.25",
    "@types/prettier": "2.7.0",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "concurrently": "7.3.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "graphql": "16.6.0",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "dependencies": {
    "@kawaiioverflow/arm": "0.101.0",
    "date-fns": "2.29.2",
    "discord.js": "13.10.3",
    "dotenv": "16.0.1",
    "fast-xml-parser": "4.0.9",
    "graphql-request": "4.3.0",
    "js-yaml": "4.1.0",
    "node-fetch": "3.2.10"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.2.2"
}
